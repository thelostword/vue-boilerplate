{
  "name": "vue-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install",
    "lint": "eslint src --ext .vue,.js,.cjs.mjs,.jsx --max-warnings 0",
    "lint:fix": "eslint src --fix --ext .vue,.js.cjs.mjs,.jsx",
    "cz": "git-cz",
    "release": "release-it"
  },
  "dependencies": {
    "@losting/ls": "^3.1.0",
    "ahooks-vue": "^0.15.0",
    "axios": "^1.4.0",
    "pinia": "^2.1.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@commitlint/cz-commitlint": "^17.5.0",
    "@release-it/conventional-changelog": "^6.0.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-airbnb": "^7.0.0",
    "commitizen": "^4.3.0",
    "husky": "^8.0.3",
    "inquirer": "^8.2.5",
    "lint-staged": "^13.2.2",
    "release-it": "^15.11.0",
    "sass": "^1.63.5",
    "unocss": "^0.53.1",
    "vite": "^4.3.9",
    "vite-plugin-vue-markdown": "^0.23.5",
    "vite-svg-loader": "^4.0.0"
  },
  "lint-staged": {
    "src/**/*.{vue,js,cjs,mjs,jsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
